        "import matplotlib.pyplot as plt\n",
        "print(2.6)\n",
        "# Data\n",
        "ages = [25, 30, 35, 40]\n",
        "heights = [145, 151, 165, 173]\n",
        "weights = [45, 55, 65, 75]\n",
        "\n",
        "# Create a figure with 3 subplots\n",
        "fig, axs = plt.subplots(1, 3, figsize=(18, 5))\n",
        "\n",
        "# Histogram: Age Distribution\n",
        "axs[0].hist(ages, bins=4, color='skyblue', edgecolor='black')\n",
        "axs[0].set_title('Distribution of Ages')\n",
        "axs[0].set_xlabel('Age (years)')\n",
        "axs[0].set_ylabel('Frequency')\n",
        "\n",
        "# Histogram: Height Distribution\n",
        "axs[1].hist(heights, bins=4, color='lightgreen', edgecolor='black')\n",
        "axs[1].set_title('Distribution of Heights')\n",
        "axs[1].set_xlabel('Height (cm)')\n",
        "axs[1].set_ylabel('Frequency')\n",
        "\n",
        "# Histogram: Weight Distribution\n",
        "axs[2].hist(weights, bins=4, color='salmon', edgecolor='black')\n",
        "axs[2].set_title('Distribution of Weights')\n",
        "axs[2].set_xlabel('Weight (kg)')\n",
        "axs[2].set_ylabel('Frequency')\n",
        "\n",
        "# Adjust layout to avoid overlap\n",
        "plt.tight_layout()\n",
        "\n",
        "# Show the plot\n",
        "plt.show()"
      ]
